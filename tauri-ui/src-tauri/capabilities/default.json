{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "store:default",
    "http:default",
    "fs:default",
    {
      "identifier": "fs:allow-read",
      "allow": ["$APPDATA/**", "$APPCONFIG/**", "$APPCONFIGDATA/**"]
    }
  ]
}
