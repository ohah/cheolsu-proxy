{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Cheolsu Proxy",
  "version": "0.1.0",
  "identifier": "com.cheolsu-proxy",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run dev",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Cheolsu Proxy",
        "width": 1440,
        "height": 1080
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": [
      "msi",
      "nsis"
    ],
    "icon": [
      "icons/logo.png"
    ],
    "windows": {
      "certificateThumbprint": "B00873577726B1BF928BCA45098267D64F0AD126",
      "digestAlgorithm": "sha256",
      "timestampUrl": "http://timestamp.comodoca.com",
      "wix": {
        "language": "ko-KR"
      },
      "nsis": {
        "installMode": "perMachine",
        "displayLanguageSelector": true
      }
    }
  }
}
